#include<iostream>

using namespace std;

#define ll long long
int a[100][100];

int f(int m,int n)
{
    if(m==0 || n==0)
    {
        if(a[m][n]==-1)
        return 0;
        return a[m][n]=1;
    }
    if(a[m][n]!=0)
    return a[m][n];
    if(a[m][n]==-1)
        return 0;
    
    return a[m][n]=f(m-1,n)+f(m,n-1);
}

int main()
{
    int m,n;
    cin>>m>>n;
    ll i;
    ll j;
    for(i=0;i<100;i++)
    {
        for(j=0;j<100;j++)
        {
            a[i][j]=0;
        }
    }
    
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>a[i][j];
        }
    }
    
    cout<<f(m-1,n-1);
    return 0;
}
